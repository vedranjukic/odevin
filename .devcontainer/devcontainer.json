{
    "name": "daytona opendevin",
    "dockerComposeFile": "docker-compose.yaml",
    "service": "app",
    "workspaceFolder": "/workspaces/daytona",
    "containerEnv": {
        "SANDBOX_USER_ID": "1000",
        "WORKSPACE_MOUNT_PATH": "/workspaces/daytona"
    },
    "features": {
		"ghcr.io/devcontainers/features/common-utils:2.4.4": {
            "installZsh": "true",
            "username": "enduser",
            "uid": "1000",
            "gid": "1000",
            "upgradePackages": "false"
        },
        "ghcr.io/devcontainers/features/go:1.3.0": {
            "version": "1.22.0"
        },
        "ghcr.io/devcontainers/features/docker-in-docker:2.11.0": {
            "moby": true,
            "version": "24.0.9"
        },
        "ghcr.io/devcontainers/features/node:1.5.0": {
            "version": "20.11.1"
        },
        "ghcr.io/devcontainers/features/dotnet:2.1.1": {
            "version": "lts",
            "installUsingApt": false
        },
        "ghcr.io/devcontainers/features/ruby:1.2.2": {},
        "ghcr.io/devcontainers/features/rust:1.2.0": {},
        "ghcr.io/devcontainers/features/php:1.1.3": {},
        "ghcr.io/devcontainers/features/python:1.6.2": {},
        "ghcr.io/devcontainers/features/java:1.5.0": {
            "installGradle": true,
            "installMaven": true
        },
		"ghcr.io/devcontainers-contrib/features/typescript:2": {}
	},
    "initializeCommand": "git clone https://github.com/OpenDevin/OpenDevin .devcontainer/opendevin || true",
	"remoteUser": "enduser",
    "forwardPorts": [9000]
}